startabs x30

// Les emplacements de stockages de nombres.
x00     x00  // 30 les deux nombres donnés
x00     x00  // 32 le résultat

// main
in    @ x30  // 34 input du premier nombre
in    @ x31  // 36 input du second nombre
jump    x3e  // 38 appel de la fonction
out   @ x32  // 3a sortie et affichage du résultat
stop    x00  // 3c arrête l'ordi en renvoyant 0

// Fonction de multiplication.
load  @ x31  // 3e on charge le second nombre (le compteur)
brz     x3a  // 40 si le résultat est nul, on arrête
sub     x01  // 42 on décrémente le compteur
store @ x31  // 44 on stocke le compteur
load  @ x32  // 46 on charge le résultat
add   @ x30  // 48 on ajoute le premier nombre
store @ x32  // 4a on stocke le nouveau résultat
jump    x3e  // 4c on recommence la boucle